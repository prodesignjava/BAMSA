  content: '';
}
.circle,
.circle::before,
.circle::after {
  -webkit-box-sizing: border-box !important;
  -moz-box-sizing: border-box !important;
  box-sizing: border-box !important;
  -webkit-transition: all .5s !important;
  transition: all .5s !important;
}

.circle {
