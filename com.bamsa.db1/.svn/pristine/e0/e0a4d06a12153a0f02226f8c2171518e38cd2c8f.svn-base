#Login queries
LOAD_USER_LOGIN = select uid,username,password from users where binary username=? and status='A'; 
LOAD_CLOCK_DETAILS = select cd.uid,cd.inip,cd.indate,cd.outip,cd.outdate,ed.empid from employeedetails ed,clocktimedetails cd where cd.uid=ed.uid and cd.uid=?;
UPDATE_CLOCK_DETAILS = update clocktimedetails set outip=?,outdate=? where uid=? and outip IS NULL;
MODIFY_EMP_DETAILS =select e.name,e.mobileno,e.dob,e.emermobileno,e.email,e.gender,e.picture from users u,employeedetails e where u.uid=e.uid and u.uid=?;
UPDATE_EMPLOYEE_DETAILS=UPDATE employeedetails SET name=?, dob=?,mobileno=?,emermobileno=?, email=?, gender=? WHERE uid=?;
LOAD_EMPLOYEE_DETAILS=select u.uid, ed.name ,ed.empid from users u , employeedetails ed where u.uid=ed.uid;
LOAD_EMPLOYEE_TASK=select et.tid,et.uid,et.empid,et.taskdescription,ed.name,et.givenby,et.deadline,et.givendate,td.status,td.reason,pd.projectname,et.tasktype from employeetasks et,employeedetails ed,taskdetails td,projectdetails pd where et.uid=? and et.givenby=ed.uid and et.tid=td.tid and et.uid=td.uid and et.npid=pd.npid;
LOAD_EMPLOYEES_UNDER_EMPLOYEE= employeedetailsProcedure
LOAD_PRESENT_EMPDETAILS=SELECT c.uid,e.name,e.empid FROM clocktimedetails c,employeedetails e where DATE(indate)=CURDATE() and c.uid=e.uid;
LOAD_ALL_EMPLOYEES=select uid,empid,name from employeedetails;
LOAD_ALL_EMPLOYEE_STATUS=SELECT et.tid,et.empid,et.taskdescription,et.deadline,et.givendate,td.status,td.percentagecompleted,td.backlogs,td.queries,pd.projectname,et.tasktype from employeetasks et, taskdetails td,projectdetails pd where et.tid=td.tid and pd.npid=et.npid  and et.givenby=? order by givendate DESC ;
UPDATE_TASK_DETAILS=UPDATE taskdetails SET status=?, percentagecompleted=?, queries=? ,backlogs=? WHERE tid=?;
UPDATE_PERCENTAGE=UPDATE taskdetails set percentagecompleted=?, status=? ,reason=? where tid=?;
LOAD_ALL_EMPLOYEE_DETAILS=select ed.empid,ed.name,ed.dob,ed.doj,ed.mobileno,ed.emermobileno,ed.email,ed.gender,ed.salary,ed.intime,ed.outtime,ed.branchname,em.streamid,em.hierarchyid,em.desigid from employeedetails ed,employeemappings em where ed.uid=em.uid and ed.uid=?;
LOAD_EMPLOYEE_PROJECT_DETAILS=select npid,projectname,projectdescription,projectdeadline,picture from projectdetails pet order by createddate DESC;
RESET_PASSWORD=update users set password=? where binary username=?;
LOAD_ALL_GRIEVANCE_DETAILS=select gd.gid,gd.uid,gd.grievancedetails,gd.status,gd.grievancesevere,gd.grievancetype,gd.mobileNo,ed.empid,ed.name,ed.mobileno  from grievancedetails gd,employeedetails ed where gd.uid=ed.uid ;
LOAD_INDIVIDUAL_TASKS_DETAILS=select et.taskdescription,et.tasktype,td.percentagecompleted,et.givendate from employeetasks et,taskdetails td where et.empid=? and et.tid=td.tid and et.uid=td.uid and et.npid=?;
DEPARTMENT_DETAILS=select ed.empid,ed.name,em.streamid,em.desigid,em.hierarchyid from employeedetails ed,employeemappings em where ed.uid=em.uid order by streamid;
REPORTING_DETAILS=select ed.empid,em.uid from employeedetails ed,employeemappings em where ed.uid=em.uid and em.streamid=6;
UPDATE_ASSET_THSTATUS=UPDATE assettickets SET thstatus='A',approvedby=?,approveddate=? WHERE atid=?;
LOAD_LICENSE_DETAILS=select ca.clid,ca.licensetag,ca.branchname,ca.softwarename,ca.seats,ca.manufacturer,ca.licensedto,ca.licensedmail,ca.createdby,ca.productkey,ca.orderno,ca.purchasecost,ca.purchasedate,ca.expirationdate,ca.notes,ca.createddate,ed.empid from companylicenses ca,employeedetails ed where ca.createdby=ed.uid;
LOAD_CONSUMABLE_DETAILS=select cc.ccid,cc.consumabletag,cc.consumablename,cc.branchname,cc.consumablecategory,cc.manufacturer,cc.createdby,cc.modelno,cc.itemno,cc.orderno,cc.purchasedate,cc.purchasecost,cc.quantity,cc.createddate,ed.empid from companyconsumables cc,employeedetails ed where cc.createdby=ed.uid;
LOAD_COMPANY_ASSETS=SELECT ca.caid,ca.assettag,ca.assetstatus,ca.branchname,ca.assetserial,ca.assetname,ca.createdby,ca.purchasedate,ca.supplier,ca.orderno,ca.purchasecost,ca.warranty,ca.notes,ca.assetimage,ca.model,ca.createddate,ed.empid FROM companyassets ca,employeedetails ed where ca.createdby=ed.uid;
LOAD_COMPANY_ACCESSORY=SELECT ca.caid,ca.accessoriestag,ca.branchname,ca.accessoryname,ca.accessorycategory,ca.manufacturer,ca.createdby,ca.modelno,ca.orderno,ca.purchasedate,ca.purchasecost,ca.quantity,ca.createddate,ed.empid FROM companyaccessories ca,employeedetails ed where ca.createdby=ed.uid;
LOAD_COMPANY_COMPONENT=SELECT cc.ccmid,cc.componenttag,cc.componentname,cc.branchname,cc.createdby,cc.category,cc.serialno,cc.orderno,cc.purchaseddate,cc.purchasecost,cc.createddate,ed.empid FROM companycomponents cc,employeedetails ed where cc.createdby=ed.uid;
LOAD_ASSET_TICKETS=SELECT at.atid,at.assettype,at.tag,at.requestto,at.purpose,at.remarks,at.fromdate,at.todate,at.risedby,at.riseddate,at.thstatus,at.approvedby,at.approveddate,at.bstatus,at.rstatus,ed.empid FROM assettickets at,employeedetails ed where at.risedby=ed.uid;
UPDATE_BSTATUS=UPDATE assettickets set bstatus='F',approvedby=?,approveddate=? where atid=?;
UPDATE_RSTATUS=UPDATE assettickets set rstatus='F',approvedby=?,approveddate=? where atid=?;
UPDATE_ASSET_THSTATUS=UPDATE assettickets SET thstatus='A',approvedby=?,approveddate=? WHERE atid=?;
LOAD_EMAILS_EMPLOYEE=SELECT email FROM employeedetails;
UPDATE_GRIEVCETICKET=UPDATE grievancedetails SET status='F' WHERE gid=?;
UPDATE_EMPLOYEE_EDIT_DETAILS=update employeemappings em,employeedetails ed  set em.hierarchyid=?,em.streamid=?,em.desigid=?,ed.salary=?,ed.branchname=?  where em.uid=ed.uid and ed.uid=?;
LOAD_PROJECT_DETAILS=SELECT npid,projectname from projectdetails;
LOAD_EMPLOYEES_UNDER_PROJECT=select et.tid,et.uid,et.empid,et.tasktype,et.npid,ed.name,ed.branchname,td.percentagecompleted from employeetasks et,employeedetails ed,taskdetails td where et.uid=ed.uid and td.tid=et.tid and et.npid=?; 
